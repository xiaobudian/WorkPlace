extends layout
block content
  form(method="post")
    | username:
    input(type="text" name="name")
    p  
    | password:
    input(type="password" name="password")
    p  
    | confrim:
    input(type="password" name="password-repeat")
    p
    | email
    input(type="email" name = "email")
    p 
    input(type="submit" value="register")